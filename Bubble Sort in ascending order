list_numbers = [1, 3, 4, 2, 6, 5, 8, 7, 9, 10, 14, 13, 20, 21, 17, 15, 19]


def sort(lst):
  for i in range(len(lst) - 1):
    for j in range(len(lst) - 1 - i):
      if lst[j + 1] < lst[j]:
        temp = lst[j]
        lst[j] = lst[j + 1]
        lst[j + 1] = temp

  return lst
  
sort(list_numbers)
